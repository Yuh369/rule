[URL Rewrite]

# 美国
(?<=_region=)CN(?=&) US 307
# 日本[需自行开启]
#(?<=_region=)CN(?=&) JP 307
# 韩国[需自行开启]
#(?<=_region=)CN(?=&) KR 307
# 中国台湾[需自行开启]
#(?<=_region=)CN(?=&) TW 307


(?<=&mcc_mnc=)4 2 307
^(https?:\/\/(tnc|dm)[\w-]+\.\w+\.com\/.+)(\?)(.+) $1$3 302
(^https?:\/\/*\.\w{4}okv.com\/.+&.+)(\d{2}\.3\.\d)(.+) $118.0$3 302

[MITM]
hostname = *.tiktokv.com,*.byteoversea.com,*.tik-tokapi.com
